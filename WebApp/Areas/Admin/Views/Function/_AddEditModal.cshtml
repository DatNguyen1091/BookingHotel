                <form class="form-horizontal" role="form" id="formMaintainance">
                    <div class="form-group">
                        <input type="hidden" class="form-control" id="hidId" name="hidId">
                        <label for="txtFunctionId" class="col-sm-auto control-label no-padding-right">FunctionId(*)</label>
                        <div class="col-sm-12">
                            <input type="text" class="form-control" id="txtFunctionId" name="txtFunctionId" placeholder="Function Id">
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="txtName" class="col-sm-auto control-label no-padding-right">Name(*)</label>
                        <div class="col-sm-12">
                            <input type="text" class="form-control" id="txtName" name="txtName" placeholder="Name">
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="txtUrl" class="col-sm-auto control-label no-padding-right">URL</label>
                        <div class="col-sm-12">
                            <input type="text" class="form-control" id="txtUrl" name="txtUrl" placeholder="URL">
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label for="txtParentId" class="col-sm-auto control-label no-padding-right">ParentId</label>
                        <div class="col-sm-12">
